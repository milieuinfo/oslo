    rdfs:isDefinedBy <https://data.vlaanderen.be/ns/subsidie> ;
    rdfs:range xsd:boolean .

<https://data.vlaanderen.be/ns/subsidie#heeftRechtsgrond> a owl:ObjectProperty ;
